Your links!

<form action="/links/new" method="gets">
  <input type="submit" value="Submit">
</form>

<div>
    <% @links.each do |link| %> <!-- Or whatever variable you would like to pass -->
        <ul id='links'>
          <li>
          <%= link.title%>
          <%= link.url %>
        </li>
    <% end %>
  </ul>
</div>
