Your links!

<form action="/new" method="gets">
  <input type="submit" value="Save a new link">
</form>

<div>
    <% @links.each do |link| %> <!-- Or whatever variable you would like to pass -->
        <ul id='links'>
          <li><%=link.title%></li>
          <li><%=link.url %></li>
          <li><%=link.tag %></li>
        </ul>
    <%end%>
</div>
